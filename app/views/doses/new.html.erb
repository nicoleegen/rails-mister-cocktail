<h2>Add a new Ingredient for <%= @cocktail.name %></h2>
      <%= simple_form_for [ @cocktail, @dose ] do |f| %>
        <%= f.input :ingredient, collection: Ingredient.all, input_html: { multiple: true, class: 'multiple-select' }, include_hidden: false %>
        <%= f.submit "Add ingredient", class: 'btn btn-primary' %>
      <% end %>

<p>Find me in app/views/doses/new.html.erb</p>
